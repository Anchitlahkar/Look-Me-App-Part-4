{
  "expo": {
    "name": "Look Me App",
    "slug": "App",
    "version": "3.0.0",
    "orientation": "portrait",
    "icon": "./assets/filter/appIcon.png",
    "splash": {
      "image": "./assets/filter/appIcon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/filter/appIcon.png",
        "backgroundColor": "#000"
      },
      "package": "com.look_me.app"
    },
    "web": {
      "favicon": "./assets/filter/appIcon.png"
    }
  }
}
